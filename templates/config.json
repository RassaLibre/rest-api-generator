{
  "name" : "NodeJS REST API template collection",
  "description" : "Template collection for creating NodeJS REST API",
  "templates" : {
    "normal" : [
      {
        "path" : "normal.tmpl.js",
        "destination" : ""
      },
      {
        "path" : "normal2/normal2.tmpl.js",
        "destination" : "normal3/"
      }
    ],
    "atomic" : [
      {
        "path" : "atomic.tmpl.js"
      }
    ],
    "duplicated" : [
      {
        "path" : "duplicated.tmpl.js",
        "scope" : "model.models",
        "reference" : "model",
        "name_property" : "name",
        "destination" : "models/"
      },
      {
        "path" : "duplicated2/duplicated2.tmpl.js",
        "scope" : "model.models",
        "reference" : "model",
        "name_property" : "name"
      }
    ]
  }
}